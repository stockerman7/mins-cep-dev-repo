<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="10.0" ExtensionBundleId="com.example.myDev" ExtensionBundleVersion="1.0.0" ExtensionBundleName="Extension-Name"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<ExtensionList>
		<Extension Id="com.example.myDev.imageList" Version="1.0" />
		<Extension Id="com.example.myDev.imageView" Version="1.0" />
	</ExtensionList>

	<ExecutionEnvironment>
		<HostList>
			<Host Name="PHXS" Version="[22.0,22.9]" />
			<Host Name="PHSP" Version="[22.0,22.9]" />
		</HostList>

		<LocaleList>
			<Locale Code="All" />
		</LocaleList>

		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="10.0" />
		</RequiredRuntimeList>

	</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.example.myDev.imageList">
			<DispatchInfo >
				<Resources>
					<MainPath>./imageList/index.html</MainPath>
					<ScriptPath>./imageList/jsx/imageList.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Image List</Menu>
					<Geometry>
						<Size>
							<Height>600</Height>
							<Width>300</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
		<Extension Id="com.example.myDev.imageView">
			<DispatchInfo >
				<Resources>
					<MainPath>./imageView/index.html</MainPath>
					<ScriptPath>./imageView/jsx/imageView.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Image View</Menu>
					<Geometry>
						<Size>
							<Height>600</Height>
							<Width>800</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>